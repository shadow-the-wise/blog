<div id="<%= dom_id article %>">
  <p>
    <strong>Title:</strong>
    <%= article.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= article.content %>
  </p>

  <p>
    <strong>Pagecount:</strong>
    <%= article.pagecount %>
  </p>

  <p>
    <strong>Published at:</strong>
    <%= article.published_at %>
  </p>

  <p>
    <strong>Published:</strong>
    <%= article.published %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= article.category_id %>
  </p>

</div>
